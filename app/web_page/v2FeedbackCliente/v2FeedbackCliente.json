  "variables" : {
    "task" : {
      "type" : "url",
      "value" : [
        "../API/bpm/userTask/{{taskId}}"
      ],
      "exposed" : false
    },
    "submit_errors_list" : {
      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.analiticaOP ){",
        "\treturn {",
        "\t\t//map analiticaOP variable to expected task contract input",
        "\t\tanaliticaOPInput: {",
        "\t\t\tobservacionesDeClientes: $data.analiticaOP.observacionesDeClientes !== undefined ? $data.analiticaOP.observacionesDeClientes : null,",
        "\t\t\tobservacionesDeOperaciones: $data.analiticaOP.observacionesDeOperaciones !== undefined ? $data.analiticaOP.observacionesDeOperaciones : null,",
        "\t\t\ttotalConsumos: $data.analiticaOP.totalConsumos !== undefined ? $data.analiticaOP.totalConsumos : null",
        "\t\t}",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "context" : {
      "type" : "url",
      "value" : [
        "../API/bpm/userTask/{{taskId}}/context"
      ],
      "exposed" : false
    },
    "analiticaOP" : {
      "type" : "url",
      "value" : [
        "../{{context.analiticaOP_ref.link}}"
      ],
      "exposed" : false
    },
    "taskId" : {
      "type" : "urlparameter",
      "value" : [
        "id"
      ],
      "exposed" : false
    },
    "informe" : {
      "type" : "expression",
      "value" : [
        "let a = $data.analiticaOP.informeConsumo;",
        "let b = JSON.parse(a);",
        "let total = 0;",
        "let info = [];",
        "",
        "b.items.map(function(val){",
        "    total += val.total;",
        "    info.push({",
        "        \"Id\": val.tipo_id + val.numero_id,",
        "        \"Nombre\": val.nombre,",
        "        \"Periodo\": val.mes_periodo.toString().padStart(2, '0') + '-' + val.anio_periodo,",
        "        \"Aplicativo\": val.aplicativo,",
        "        \"Numero_Consultas\": val.total_consultas_exitosas,",
        "        \"Costo_por_Consulta\": val.promedio_precio_por_consulta.toLocaleString('es-CO', { style: 'currency', currency: 'COP' }),",
        "        \"Subtotal\": val.total.toLocaleString('es-CO', { style: 'currency', currency: 'COP' })",
        "    });",
        "});",
        "",
        "info.push({\"Id\": \"<strong>TOTAL</strong>\",",
        "        \"Nombre\": \"\",",
        "        \"Periodo\": \"\",",
        "        \"Aplicativo\": \"\",",
        "        \"Numero_Consultas\": \"\",",
        "        \"Costo_por_Consulta\": \"\",",
        "        \"Subtotal\": \"<strong>\"+total.toLocaleString('es-CO', { style: 'currency', currency: 'COP' })})+\"</strong>\";",
        "        ",
        "$data.totalConsumos = total.toLocaleString('es-CO', { style: 'currency', currency: 'COP' });",
        "return info;"
      ],
      "exposed" : false
    },
    "totalConsumos" : {
      "type" : "constant",
      "exposed" : false
    }
  },
