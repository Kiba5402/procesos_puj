    },
    "submit_errors_list" : {
      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.solicitudInforme ){",
        "\treturn {",
        "\t\t//map solicitudInforme variable to expected task contract input",
        "\t\tsolicitudInformeInput: {",
        "\t\t\tidentificadorProceso: $data.solicitudInforme.identificadorProceso !== undefined ? $data.solicitudInforme.identificadorProceso : null,",
        "\t\t\tbajaEnConsumos: $data.solicitudInforme.bajaEnConsumos !== undefined ? $data.solicitudInforme.bajaEnConsumos : null,",
        "\t\t\tclienteAConsultar: $data.solicitudInforme.clienteAConsultar !== undefined ? $data.solicitudInforme.clienteAConsultar : null",
        "\t\t}",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "context" : {
      "type" : "url",
      "value" : [
        "../API/bpm/userTask/{{taskId}}/context"
      ],
      "exposed" : false
    },
    "taskId" : {
      "type" : "urlparameter",
      "value" : [
        "id"
      ],
      "exposed" : false
    },
    "selectCliente" : {
      "type" : "expression",
      "value" : [
        "let a = $data.solicitudInforme.listaClientes",
        "let clientes = JSON.parse(a);",
        "let select = [];",
        "",
        "clientes.items.map(function(value){",
        "    select.push(value.nombre);",
        "});",
        "",
        "return select;"
      ],
      "exposed" : false
    }
