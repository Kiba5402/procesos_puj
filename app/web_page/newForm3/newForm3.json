    "submit_errors_list" : {
      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.coordinacionTI ){",
        "\treturn {",
        "\t\t//map coordinacionTI variable to expected task contract input",
        "\t\tcoordinacionTIInput: {",
        "\t\t\tfechaRequerimiento: $data.coordinacionTI.fechaRequerimiento !== undefined ? $data.coordinacionTI.fechaRequerimiento : null,",
        "\t\t\tfechaInicioPeriodoAnalisis: $data.coordinacionTI.fechaInicioPeriodoAnalisis !== undefined ? $data.coordinacionTI.fechaInicioPeriodoAnalisis : null,",
        "\t\t\tfechaFinalPeriodoAnalisis: $data.coordinacionTI.fechaFinalPeriodoAnalisis !== undefined ? $data.coordinacionTI.fechaFinalPeriodoAnalisis : null,",
        "\t\t\taplicacionesAConsultar: $data.coordinacionTI.aplicacionesAConsultar !== undefined ? $data.coordinacionTI.aplicacionesAConsultar : null,",
        "\t\t\tinformeConsumo: $data.coordinacionTI.informeConsumo !== undefined ? $data.coordinacionTI.informeConsumo : null,",
        "\t\t\tdetalleSolicitud: $data.coordinacionTI.detalleSolicitud !== undefined ? $data.coordinacionTI.detalleSolicitud : null",
        "\t\t}",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
