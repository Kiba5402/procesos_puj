    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.coordinacionTI ){",
        "\treturn {",
        "\t\t//map coordinacionTI variable to expected task contract input",
        "\t\tcoordinacionTIInput: {",
        "\t\t\tfechaRequerimiento: $data.coordinacionTI.fechaRequerimiento !== undefined ? $data.coordinacionTI.fechaRequerimiento : null,",
