        "\t\t\tfechaInicioPeriodoAnalisis: $data.financiera.fechaInicioPeriodoAnalisis !== undefined ? $data.financiera.fechaInicioPeriodoAnalisis : null,",
        "\t\t\tfechaFinalPeriodoAnalisis: $data.financiera.fechaFinalPeriodoAnalisis !== undefined ? $data.financiera.fechaFinalPeriodoAnalisis : null,",
        "\t\t\tpaqueteEstrategiasFidelizacion: $data.financiera.paqueteEstrategiasFidelizacion !== undefined ? $data.financiera.paqueteEstrategiasFidelizacion : null,",
        "\t\t\tpaqueteEstrategiasRetencion: $data.financiera.paqueteEstrategiasRetencion !== undefined ? $data.financiera.paqueteEstrategiasRetencion : null,",
        "\t\t\tobservacionesFinancieras: $data.financiera.observacionesFinancieras !== undefined ? $data.financiera.observacionesFinancieras : null,",
        "\t\t\tpaqueteAprobado: $data.financiera.paqueteAprobado !== undefined ? $data.financiera.paqueteAprobado : null",
        "\t\t}",
        "\t}",
        "}"
      ],
      "exposed" : false
