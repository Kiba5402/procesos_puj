        "\treturn {",
        "\t\t//map solicitudInforme variable to expected task contract input",
        "\t\tsolicitudInformeInput: {",
        "\t\t\tidProceso: $data.solicitudInforme.idProceso !== undefined ? $data.solicitudInforme.idProceso : null,",
